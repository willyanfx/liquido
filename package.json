{
    "name": "liquid",
    "displayName": "Liquido",
    "description": "shopify liquid support",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "liquid",
                "aliases": [
                    "html.liquid",
                    "liquid"
                ],
                "extensions": [
                    "liquid"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "liquid",
                "scopeName": "source.liquid",
                "path": "./syntaxes/liquid.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.json": "json"
                }
            }
        ],
        "snippets": [
            {
                "language": "liquid",
                "path": "./snippets/liquid.json"
            }
        ]
    }
}
